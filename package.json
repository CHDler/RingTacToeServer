    "@colyseus/loadtest": "^0.16.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^2.7.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^3.4.5"
  },
"dependencies": {
  "colyseus": "^0.17.8",
